<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white">

    <androidx.cardview.widget.CardView
        android:id="@+id/roundedBackgroundTiki"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:visibility="invisible"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        app:cardCornerRadius="35dp"
        app:cardElevation="8dp"
        app:cardBackgroundColor="@color/space"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <TextView
            android:id="@+id/textCourierTiki"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TIKI"
            android:fontFamily="@font/poppins_bold"
            android:textSize="12sp"
            android:textColor="@color/yankes_blue"
            android:layout_gravity="center"
            android:gravity="center"/>

    </androidx.cardview.widget.CardView >

    <androidx.cardview.widget.CardView
        android:id="@+id/roundedBackgroundPos"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:visibility="invisible"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        app:cardCornerRadius="35dp"
        app:cardElevation="8dp"
        app:cardBackgroundColor="@color/blue"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <TextView
            android:id="@+id/textCourierPos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="POS"
            android:fontFamily="@font/poppins_bold"
            android:textSize="12sp"
            android:textColor="@color/yankes_blue"
            android:layout_gravity="center"
            android:gravity="center"/>

    </androidx.cardview.widget.CardView >

    <androidx.cardview.widget.CardView
        android:id="@+id/roundedBackgroundJne"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:visibility="invisible"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        app:cardCornerRadius="35dp"
        app:cardElevation="8dp"
        app:cardBackgroundColor="@color/green"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <TextView
            android:id="@+id/textCourierJne"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="JNE"
            android:fontFamily="@font/poppins_bold"
            android:textSize="12sp"
            android:textColor="@color/yankes_blue"
            android:layout_gravity="center"
            android:gravity="center"/>

    </androidx.cardview.widget.CardView >

    <TextView
        android:id="@+id/jasaOngkir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginStart="10dp"
        android:text="Text Jasa Ongkir"
        android:fontFamily="@font/poppins_bold"
        android:textSize="14sp"
        android:textColor="@color/cool_blue"
        android:layout_gravity="center"
        android:gravity="center"
        app:layout_constraintStart_toEndOf="@id/roundedBackgroundTiki"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/typeOngkir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="Text Type Ongkir"
        android:fontFamily="@font/poppins_regular"
        android:textSize="12sp"
        android:textColor="@color/textfield"
        android:layout_gravity="center"
        android:gravity="center"
        app:layout_constraintStart_toEndOf="@id/roundedBackgroundTiki"
        app:layout_constraintTop_toBottomOf="@id/jasaOngkir"/>

    <TextView
        android:id="@+id/priceOngkir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="Text Harga Ongkir"
        android:fontFamily="@font/poppins_regular"
        android:textSize="14sp"
        android:textColor="@color/error"
        android:layout_gravity="center"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/rangeOngkir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="hari"
        android:fontFamily="@font/poppins_regular"
        android:textSize="12sp"
        android:textColor="@color/textfield"
        android:layout_gravity="center"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/priceOngkir"/>

    <View
        android:id="@+id/line_2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentTop="true"
        app:layout_constraintTop_toBottomOf="@id/roundedBackgroundTiki"
        android:background="@color/card" />

</androidx.constraintlayout.widget.ConstraintLayout>